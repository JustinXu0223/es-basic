{
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "plugins": [
  ],
  "env": {
    "es6": true,
    "jquery": true,
    "browser": true,
    "node": true,
    "commonjs": true,
    "mocha": true
  },
  "rules": {
    "import/prefer-default-export": 0,
    "function-paren-newline": 0,
    "max-len": [
      "error",
      { "code": 200 }
    ],
    "consistent-return": [0],
    "no-param-reassign": [0]
  }
}
