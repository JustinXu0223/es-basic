{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "jquery": true,
    "browser": true,
    "node": true,
    "commonjs": true,
    "mocha": true
  },
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "jsx-a11y/no-static-element-interactions": 0,
    "import/prefer-default-export": 0,
    "function-paren-newline": 0,
    "jsx-a11y/anchor-is-valid": [
      "error",
      {
        "components": 0
      }
    ],
    "max-len": [
      "error",
      { "code": 200 }
    ],
    "consistent-return": [0],
    "no-param-reassign": [0]
  }
}
